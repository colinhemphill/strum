{
  "name": "@strum/react",
  "version": "1.2.1",
  "description": "A React design system built with Vanilla Extract",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colinhemphill/strum.git"
  },
  "homepage": "https://strum.design",
  "keywords": [
    "design system",
    "vanilla extract",
    "strum",
    "react",
    "components",
    "styles"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prepack": "pnpm build",
    "publish": "pnpm npm publish",
    "test": "jest",
    "types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-accessible-icon": "1.0.1",
    "@radix-ui/react-avatar": "1.0.1",
    "@radix-ui/react-checkbox": "1.0.1",
    "@radix-ui/react-icons": "1.1.1",
    "@radix-ui/react-label": "2.0.0",
    "@radix-ui/react-radio-group": "1.1.0",
    "@radix-ui/react-select": "1.1.1",
    "@radix-ui/react-toast": "1.1.2",
    "@radix-ui/react-tooltip": "1.0.2",
    "@vanilla-extract/css": "1.9.2",
    "@vanilla-extract/css-utils": "0.1.3",
    "@vanilla-extract/recipes": "0.3.0",
    "@vanilla-extract/sprinkles": "1.5.1",
    "clsx": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@strum/eslint-config": "*",
    "@strum/tsconfig": "*",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@vanilla-extract/babel-plugin": "1.2.0",
    "@vanilla-extract/vite-plugin": "3.7.0",
    "eslint": "8.30.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "polished": "4.2.2",
    "react": ">=17",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "ts-deepmerge": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "vite": "3.1.4",
    "vite-plugin-dts": "1.6.6"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}